# top-most EditorConfig file
root = true

[*]
end_of_line = lf
insert_final_newline = true
trim_trailing_whitespace = true

[**.php]
charset = utf-8
indent_style = space
indent_size = 4

[**.{less|css}]
charset = utf-8
indent_style = space
indent_size = 4

[**.js]
charset = utf-8
indent_style = space
indent_size = 4
